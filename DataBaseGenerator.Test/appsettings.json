{
  "Logging": {
    "Files": {
      "Emulator": {
        "Path": "Logs/Emulator.log",
        "MaxFlushInterval": 0,
        "FlushToDisk": true,
        "FormatterOptions": {
          "SingleLine": true,
          "EmptyLineBetweenMessages": false,
          "TimestampFormat": "yyyy-MM-dd HH:mm:ss.fff"
        },
        "LogLevel": {
          "Default": "Trace",
          "AutoTestSuite": "Trace",
          "DataBaseTestClient": "Trace"
        }
      },
      "Devices": {
        "Path": "Logs/Devices.log",
        "MaxFlushInterval": 0,
        "FlushToDisk": true,
        "FormatterOptions": {
          "SingleLine": true,
          "EmptyLineBetweenMessages": false,
          "TimestampFormat": "yyyy-MM-dd HH:mm:ss.fff"
        },
        "LogLevel": {
          "Default": "None",
          "Generator": "None",
          "Detector": "None",
          "ControlBlock": "None",
          "Camera": "None"
        }
      }
    }
  }
}
